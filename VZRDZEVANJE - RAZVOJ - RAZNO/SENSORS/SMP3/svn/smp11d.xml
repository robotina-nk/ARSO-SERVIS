<?xml version="1.0" encoding="UTF-8"?>
<smp11d>
<File>
	<Description>Main amws Sensor Module Application - smp11 Daemon - config file</Description>
	<Comment>File is loaded at startup. Changes require the restart of the Application.</Comment>
</File>
<Settings>
	<Watchdog Type="None" opt_Type="None, Moxa" Timeout="80000" Unit="ms">
	</Watchdog>
	<Log Level="Debug" LevelFilter="Debug" opt_Level="None,Error,Info,Device,Debug"  BufferSizeEvents="300"
	Type="File" opt_Type="None,File" FileName="./smp11d.log" FileSizeKB="4096" >
	</Log>
	<Driver Family="smp11d" xmlConfig="smp11d.h.xml">
		<Memory>
			<Settings MemoryIntervals="2983" MemoryIntervals_Unit="kB" MemorySampling="3242" MemorySampling_Unit="kB" optimizeEnable="true"/>
			<Backup MemoryBackupFolder="smp11d_backup" MemoryBackupLimit="1440" />
		</Memory>
		<Terminal>
			<Type TerminalType="Admin" opt_TerminalType="Limited, Calib, Config, Admin"/>
			<Security TerminalSecurity="none" TerminalSecurityData="terminal_users.h.xml" opt_TerminalSecurity="none, basic"/>
			<Settings TerminalTimeout_s="120" TerminalPort="64005" />
		</Terminal>
	</Driver>
</Settings>
<Station xmlConfig="../system.h.xml">
</Station>
</smp11d>
